{
  "name": "roboskills",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "dotenv -e .env -- npx prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "prisma:generate": "dotenv -e .env -- npx prisma generate",
    "prisma:push": "dotenv -e .env -- npx prisma db push",
    "prisma:migrate": "dotenv -e .env -- npx prisma migrate dev --name update",
    "prisma:reset": "dotenv -e .env -- npx prisma migrate reset --force",
    "prisma:status": "dotenv -e .env -- npx prisma migrate status"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.19.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.4.0",
    "@studio-freight/lenis": "^1.0.42",
    "@tailwindcss/oxide": "^4.1.16",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.9.1",
    "bcrypt": "^6.0.0",
    "framer-motion": "^12.23.24",
    "gsap": "^3.13.0",
    "lightningcss": "^1.30.2",
    "next": "15.5.6",
    "next-auth": "^4.24.13",
    "nodemailer": "^7.0.10",
    "pg": "^8.16.3",
    "prisma": "^6.19.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "react-masonry-css": "^1.0.16",
    "sass": "^1.93.3",
    "scss": "^0.2.4",
    "three": "^0.181.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "24.10.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.6",
    "tailwindcss": "^4"
  }
}
